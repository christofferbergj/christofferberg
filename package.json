{
  "name": "luke-gatsby-starter",
  "description": "An opinionated GatsbyJS starter using TypeScript and React String.",
  "starter-name": "luke-gatsby-starter",
  "version": "1.1.1",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "react",
    "typescript",
    "gatsby-starter"
  ],
  "contributors": [
    "Luke Secomb <dev@lukesecomb.digital>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lukethacoder/luke-gatsby-starter"
  },
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "start": "gatsby serve --port ${PORT:-3000}",
    "clean": "rimraf .cache public",
    "lint": "prettier --write && stylelint --fix"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.10.1",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.18.7",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-config": "^1.0.7",
    "gatsby-plugin-emotion": "^4.1.16",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-nprogress": "^2.1.17",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-transition-link": "^1.17.7",
    "gatsby-plugin-typescript": "^2.1.12",
    "gatsby-transformer-sharp": "^2.3.7",
    "intersection-observer": "^0.7.0",
    "prismjs": "^1.27.0",
    "react-dom": "^16.11.0",
    "react": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@types/react-helmet": "^5.0.14",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "gatsby-plugin-sitemap": "^2.2.19",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "rimraf": "3.0.0",
    "stylelint": "11.1.1",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,jsx,js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  }
}
